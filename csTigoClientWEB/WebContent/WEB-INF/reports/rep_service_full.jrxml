	<queryString>
		<![CDATA[SELECT 	  u.CELLPHONE_NUM,
	  u.NAME_CHR,
          s.DESCRIPTION_CHR,
	  m.DATEIN_DAT AS HORA
FROM SERVICE_VALUE sv
	INNER JOIN USERPHONE u ON (sv.COD_USERPHONE = u.USERPHONE_COD)
	INNER JOIN MESSAGE m ON (sv.COD_MESSAGE = m.MESSAGE_COD)
        INNER JOIN SERVICE s ON (sv.COD_SERVICE = s.SERVICE_COD)
WHERE 1 = 1 $P!{WHERE}
$P!{ORDER_BY}]]>
	</queryString>
