	<queryString>
		<![CDATA[SELECT 	  m.DATEIN_DAT HORA,
	  sv.SERVICEVALUE_COD,
	  u.CELLPHONE_NUM,
	  u.NAME_CHR,
	  sv.RECORDDATE_DAT
FROM SERVICE_VALUE sv
	INNER JOIN USERPHONE u ON (sv.COD_USERPHONE = u.USERPHONE_COD)
	INNER JOIN MESSAGE m ON (m.MESSAGE_COD = sv.COD_MESSAGE)
WHERE 1 = 1
AND   sv.COD_SERVICE = 4 $P!{WHERE}
$P!{ORDER_BY}]]>
	</queryString>
