	<queryString>
		<![CDATA[SELECT svd.SERVICEVALUEDETAIL_COD CODDET,
          svd.COLUMN1_CHR COD_CLIENTE,
          CASE WHEN mc.ENABLED_CHR = '1' THEN md.value_chr ELSE t.DATA_C END CLIENTE,
          svd.COLUMN2_CHR SUPER,
          CASE WHEN svd.COLUMN3_CHR = '1' THEN 'TB' ELSE 'D' END TIPOBANDEJA,
          -- svd.COLUMN4_CHR UBICACION,
          svd.COLUMN5_CHR CANTIDAD,
          CASE WHEN svd.COLUMN4_CHR = '1' THEN 'Camara' ELSE 'Vacias' END UBICACION,
          sv.SERVICEVALUE_COD CODCAB,
          u.CELLPHONE_NUM NROMOVIL,
          u.NAME_CHR NOMBRE,
          svd.RECORDDATE_DAT FECHA

FROM SERVICE_VALUE_DETAIL svd
        INNER JOIN SERVICE_VALUE sv ON (sv.SERVICEVALUE_COD = svd.COD_SERVICEVALUE)
        INNER JOIN USERPHONE u ON (sv.COD_USERPHONE = u.USERPHONE_COD)
        INNER JOIN MESSAGE m ON (sv.COD_MESSAGE = m.MESSAGE_COD)
LEFT OUTER JOIN TMP_WSRESULT t ON (TRIM(t.DATA_ID) = TRIM(svd.COLUMN1_CHR) AND t.DATA_TYPE = 'C' AND t.SESSION_ID = '$P!{SESSION_ID}' )

        LEFT JOIN META_DATA md ON (md.cod_client = u.COD_CLIENT AND md.cod_meta = 1 AND md.cod_member = 1 AND md.CODE_CHR = svd.COLUMN1_CHR)
        LEFT JOIN META_CLIENT mc ON (mc.cod_client = u.COD_CLIENT AND mc.COD_META = 1)

WHERE 1 = 1 AND sv.COD_SERVICE = 8
$P!{WHERE}
$P!{ORDER_BY}]]>
	</queryString>
