	<queryString>
		<![CDATA[SELECT o.SERVICEVALUEDETAIL_COD COD,
	  o.COLUMN1_CHR COD_PRODUCTO,
	  CASE WHEN mc2.ENABLED_CHR = '1' THEN md2.value_chr ELSE t.DATA_C END PRODUCTO,
	  o.COLUMN2_CHR CANT,
	  o.COLUMN3_CHR DESCUENTO,
	  o.COLUMN4_CHR PU,
	  o.COLUMN5_CHR TOTAL_PRODUCTO,
            o1.COLUMN2_CHR COND_VENTA,
	  o1.COLUMN3_CHR LISTA_PRECIO
FROM SERVICE_VALUE_DETAIL o
INNER JOIN SERVICE_VALUE o1 ON (o.COD_SERVICEVALUE = o1.SERVICEVALUE_COD)
INNER JOIN USERPHONE u ON (o1.COD_USERPHONE = u.USERPHONE_COD)
LEFT OUTER JOIN TMP_WSRESULT t ON (TRIM(t.DATA_ID) = TRIM(o.COLUMN1_CHR) AND t.DATA_TYPE = 'P' AND t.SESSION_ID = '$P!{SESSION_ID}')
LEFT JOIN META_DATA md2 ON (md2.cod_client = u.COD_CLIENT AND md2.cod_meta = 2 AND md2.cod_member = 1 AND md2.CODE_CHR = o.COLUMN1_CHR)
LEFT JOIN META_CLIENT mc2 ON (mc2.cod_client = u.COD_CLIENT AND mc2.COD_META = 2)
WHERE 1 = 1 $P!{WHERE}
$P!{ORDER_BY}]]>
	</queryString>
