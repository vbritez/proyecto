	<queryString>
		<![CDATA[SELECT cl.DESCRIPTION_CHR,
cl.CLASSIFICATION_COD,
	a.auditory_cod as nroaudit,
	a.auditdate_dat as hourtime,
	coalesce(uw.name_chr, '') as username,
	a.action_chr as action,
	a.ipaddress as ipaddress

FROM auditory a
	LEFT JOIN userweb uw ON (a.cod_userweb = uw.userweb_cod)
	INNER join USERWEB_CLASSIFICATION uc on (uc.COD_USERWEB = uw.USERWEB_COD)
         	INNER join CLASSIFICATION cl on (uc.COD_CLASSIFICATION = cl.CLASSIFICATION_COD)

WHERE 1 = 1
and a.COD_USERADMIN is null
$P!{WHERE}
$P!{ORDER_BY}]]>
	</queryString>
