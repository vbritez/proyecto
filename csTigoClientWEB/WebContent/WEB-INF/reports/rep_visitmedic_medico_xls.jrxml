	<queryString>
		<![CDATA[select sv.COLUMN2_CHR COD_CLINIC,
CASE WHEN mc2.ENABLED_CHR = '1' THEN md2.value_chr ELSE null END clinic,
m.DATEIN_DAT fecha
from CSTIGO.SERVICE_VALUE sv
inner join CSTIGO.USERPHONE u on u.USERPHONE_COD = sv.COD_USERPHONE
inner join CSTIGO.MESSAGE m on sv.COD_MESSAGE = m.MESSAGE_COD
--MEDIC
     LEFT JOIN META_DATA md2 ON (md2.cod_client = u.COD_CLIENT AND md2.cod_meta = 15 AND md2.cod_member = 1 AND md2.CODE_CHR = sv.COLUMN2_CHR)
     LEFT JOIN META_CLIENT mc2 ON (mc2.cod_client = u.COD_CLIENT AND mc2.COD_META = 15)
where 1 = 1
and sv.COD_SERVICE = 17
 $P!{WHERE}
$P!{ORDER_BY}]]>
	</queryString>
